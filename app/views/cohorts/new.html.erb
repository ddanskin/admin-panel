<%= link_to("<< Back to List", cohorts_path, :class =>'back-link') %>
<div class="cohorts new">
    <h2>Create Cohort</h2>
    <%= form_for(@cohort) do |f| %>
        <table summary="Cohort form fields">
            <tr>
                <th>Title</th>
                <td><%= f.text_field(:title) %></td>
            </tr>
            <tr>
                <th>Start Date</th>
                <td><%= f.text_field(:start_date) %></td>
            </tr>
            <tr>
                <th>End Date</th>
                <td><%= f.text_field(:end_date) %></td>
            </tr>
            <tr>
                <th>Counselor Id</th>
                <td><%= f.text_field(:employee_id) %></td>
            </tr>
        </table>
        <div class="form-buttons">
            <%= f.submit("Create Cohort") %>
        </div>
    <% end %>
</div>
