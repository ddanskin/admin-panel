<%= link_to("<< Back to List", cohorts_path, :class => 'back-link') %>

<div class="cohorts show">
    <h3>Show Cohort</h3>

    <table summary="Cohort detail view">
        <tr>
            <th>Title</th>
            <td><%= @cohort.title %></td>
        </tr>
        <tr>
            <th>Start Date</th>
            <td><%= @cohort.start_date %></td>
        </tr>
        <tr>
            <th>End Date</th>
            <td><%= @cohort.end_date %></td>
        </tr>
        <tr>
            <th>Counselor Id</th>
            <td><%= @cohort.employee_id %></td>
        </tr>
    </table>
</div>
