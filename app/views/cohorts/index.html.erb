<div class="cohorts index">
    <h3>Cohorts</h3>

    <%= link_to("Add new cohort", '#', :class => 'action new') %>

    <table class="listing" summary="Cohort list">
        <tr class="header">
            <th>Title</th>
            <th>Start Date</th>
            <th>End Date</th>
            <th>Counselor Id</th>
        </tr>
        <% @cohorts.each do |cohort| %>
            <tr>
                <td><%= cohort.title %></td>
                <td><%= cohort.start_date %></td>
                <td><%= cohort.end_date %></td>
                <td><%= cohort.employee_id %></td>
            </tr>
        <% end %>
    </table>
</div>


