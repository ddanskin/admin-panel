
        <div class="students directory col s12 l8">
            <table class="listing" summary="Student list">
                <tr class="header">
                    <th>Last Name</th>
                    <th>First Name</th>
                    <th>Cohort Id</th>
                    <th>Age</th>
                    <th>Education Level</th>
                    <th>Email</th>
                    <th>Phone Number</th>
                </tr>
                <% @students.each do |student| %>
                <tr>
                    <td><%= student.last_name %></td>
                    <td><%= student.first_name %></td>
                    <td><%= student.cohort_id %></td>
                    <td><%= student.age %></td>
                    <td><%= student.education %></td>
                    <td><%= student.email %></td>
                    <td><%= student.phone_number %></td>
                    <td class="actions">
                        <%= link_to("Show", student_path(student), :class => 'action show') %>
                        <%= link_to("Edit", edit_student_path(student), :class => 'action edit') %>
                        <%= link_to("Delete", delete_student_path(student), :class => 'action delete') %>
                    </td>
                </tr>
            <% end %>
        </table>
    </div>
