<table summary="Student form fields">
    <tr>
        <th>First Name</th>
        <td><%= f.text_field(:first_name) %></td>
    </tr>
    <tr>
        <th>Last Name</th>
        <td><%= f.text_field(:last_name) %></td>
    </tr>
    <tr>
        <th>Cohort Id</th>
        <td><%= f.number_field(:cohort_id) %></td>
    </tr>
    <tr>
        <th>Birthday</th>
        <td><%= f.number_field(:birthday) %></td>
    </tr>
    <tr>
        <th>Education level</th>
        <td><%= f.text_field(:education) %></td>
    </tr>
    <tr>
        <th>Email</th>
        <td><%= f.text_field(:email) %></td>
    </tr>
    <tr>
        <th>Phone Number</th>
        <td><%= f.text_field(:phone_number) %></td>
    </tr>
</table>
